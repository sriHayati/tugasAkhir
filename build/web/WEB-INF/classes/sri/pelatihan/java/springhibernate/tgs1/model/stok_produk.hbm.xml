<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="sri.pelatihan.java.springhibernate.tgs1.model.stok_produk" table="stok_produk_tbl">
        <id name="kode_produk" column="kode_produk" type="string">
            <generator class="assigned" />
        </id>
        <property name="stok_gudang" column="stok_gudang"/>
        <property name="stok_toko" column="stok_toko" />
        <property name="total_stok" column="total_stok" />
        
    </class>
  

</hibernate-mapping>
