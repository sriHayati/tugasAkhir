<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="sri.pelatihan.java.springhibernate.tgs1.model.delivery" table="delivery_tbl">
        <id name="kode_delivery" column="kode_delivery" type="string">
            <generator class="assigned" />
        </id>
        <property name="tanggal_delivery" column="tanggal_delivery"/>
        <property name="keterangan" column="keterangan" />
        <property name="kode_produk" column="kode_produk" />
        <property name="jumlah" column="jumlah" />
    </class>
  

</hibernate-mapping>
