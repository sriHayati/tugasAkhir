<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="sri.pelatihan.java.springhibernate.tgs1.model.pengeluaran" table="pengeluaran_tbl">
        <id name="kode_pengeluaran" column="kode_pengeluaran" type="string">
            <generator class="assigned" />
        </id>
        <property name="tanggal" column="tanggal"/>
        <property name="kode_akun" column="kode_akun"/>
        <property name="deskripsi" column="deskripsi"/>
        <property name="jumlah" column="jumlah"/>
       
    </class>
  

</hibernate-mapping>
