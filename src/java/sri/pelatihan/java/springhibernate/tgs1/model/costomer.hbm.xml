<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="sri.pelatihan.java.springhibernate.tgs1.model.customer" table="customer_tbl">
        <id name="kode_customer" column="kode_customer" type="string">
            <generator class="assigned" />
        </id>
        <property name="tanggal_penjualan" column="tanggal_penjualan"/>
        <property name="nama_customer" column="nama_customer" />
        <property name="alamat_customer" column="alamat_customer" />
        
    </class>
  

</hibernate-mapping>
